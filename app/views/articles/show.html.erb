<h1><%= @article.title %></h1>

<p><%= @article.body %></p>
<p>Ultimo acesso em: <%= Time.zone.now %></p>
<p>Arquivo atualizado em: <%= @article.updated_at %></p>

<ul>
  <li><%= link_to 'Excluir', article_path(@article),
    method: :delete,
    data: { confirm: 'Voce tem certeza?' }
  %></li>
  <li><%= link_to 'Editar', edit_article_path(@article)%></li>
  <li><%= link_to 'Voltar', root_path %></li>
</ul>

